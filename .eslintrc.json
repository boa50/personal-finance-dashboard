{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/react-in-jsx-scope": "off",
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "consistent-return": 2,
    "indent": [
      1,
      4
    ],
    "no-else-return": 1,
    "semi": [
      1,
      "never"
    ],
    "space-unary-ops": 2
  }
}